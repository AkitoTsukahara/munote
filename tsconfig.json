{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "strictNullChecks": false,
    "rootDir": "./",
    "paths": {
      "$api": ["api"],
      "$api/*": ["api/*"],
      "$lib": ["src/lib"],
      "$lib/*": ["src/lib/*"]
    }
  }
}
